{
  "description": "[dvorak] Ctrl+; to Ctrl+u for MS Excel",
  "manipulators": [
    {
      "conditions": [
        {
          "bundle_identifiers": ["^com\\.microsoft\\.Excel$"],
          "type": "frontmost_application_if"
        }
      ],
      "from": {
        "key_code": "z",
        "modifiers": { "mandatory": ["control"] }
      },
      "to": [
        {
          "key_code": "f",
          "modifiers": ["control"]
        }
      ],
      "type": "basic"
    }
  ]
}
