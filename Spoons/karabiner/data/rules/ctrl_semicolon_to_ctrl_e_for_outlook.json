{
  "description": "Ctrl+; to Ctrl+e for MS Outlook",
  "manipulators": [
    {
      "conditions": [
        {
          "bundle_identifiers": ["^com\\.microsoft\\.Outlook$"],
          "type": "frontmost_application_if"
        }
      ],
      "from": {
        "key_code": "z",
        "modifiers": { "mandatory": ["control"] }
      },
      "to": [
        {
          "key_code": "d",
          "modifiers": ["control"]
        }
      ],
      "type": "basic"
    }
  ]
}
